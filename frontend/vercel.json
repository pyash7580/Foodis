{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "env": {
        "DISABLE_ESLINT_PLUGIN": "true",
        "SKIP_PREFLIGHT_CHECK": "true",
        "GENERATE_SOURCEMAP": "false",
        "CI": "false",
        "REACT_APP_API_URL": ""
    }
}